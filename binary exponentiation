//python
a=int(input())
b=int(input())
res=1
while(b):
    if b%2!=0:
        b=b-1
        res=res*a
    else:
        b=b//2
        a=a*a
print(res)

//c
#include<stdio.h>
int main()
{
	int a,b;
	scanf("%d",&a);
	scanf("%d",&b);
	int res=1;
	while(b>0)
	{
		if(b%2!=0)
		{
			res=res*a;
			b=b-1;
		}
		else
		{
			b=b/2;
			a=a*a;
		}
	}
	printf("%d",res);
	return 0;
}
