def linear_search(arr,x,n):
    for i in range(0,n):
        if arr[i]==x:
            return i
    return -1

arr=list(map(int,input().split()))
x=int(input())
n=len(arr)
result=linear_search(arr,x,n)
if(result==-1):
    print("element not found")
else:
    print("element found at index place",result)
